<%= render 'shared/pages/navigation' %>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-content">
          <table class="table table-striped table-bordered table-hover dataTables-example">
            <thead>
            <tr>
              <th>
                <h3>Có <%= @array_of_result_objects.flatten.count %> kết quả tìm kiếm cho từ khoá: <span class="text-danger"><%= params["search"] %></span></h3>
              </th>
            </tr>
            </thead>
            <tbody>
            <% @array_of_result_objects.flatten.each do |array_of_result_object| %>
              <tr class="gradeX">
                <td><%= render 'admin/administrators/result', object: array_of_result_object %></td>
              </tr>
            <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<%= render 'shared/pages/footer' %>